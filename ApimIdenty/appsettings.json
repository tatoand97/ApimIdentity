{
  "Apim": {
    "BaseUrl": "https://<your-apim-name>.azure-api.net/",
    "Scope": "api://<apim-app-registration-client-id>/.default",
    "SampleEndpoint": "/your-api/endpoint"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
